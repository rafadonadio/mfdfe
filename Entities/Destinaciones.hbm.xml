<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Entities.Destinaciones, Entities" table="strg_destinaciones">
    <id name="Id" type="Int32" unsaved-value="0">
      <column name="id_destinacion" not-null="true" unique="true" />
      <generator class="native" />
    </id>
    <property name="Codigo" column="codigo" type="String" />
    <property name="Descripcion" column="descripcion" type="String"/>
    <property name="Desde" column="desde" type="DateTime"/>
    <property name="Hasta" column="hasta" type="DateTime"/>
  </class>
</hibernate-mapping>